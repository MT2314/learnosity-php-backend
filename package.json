{
  "name": "demo-component-library",
  "version": "1.0.0",
  "engines": {
    "npm": "6.14.17",
    "node": "14.19.2"
  },
  "scripts": {
    "build:qa": "webpack --mode production --env ENVIRONMENT=qa",
    "build:dev": "webpack --mode production --env ENVIRONMENT=dev",
    "build:prod": "webpack --mode production --env ENVIRONMENT=prod",
    "build:stage": "webpack --mode production --env ENVIRONMENT=stage",
    "build:start": "cd dist && PORT=3001 npx serve",
    "build-templates": "mkdir -p ./dist/templates && cp -R -a ./src/templates ./dist",
    "start": "webpack serve --open --mode development --env ENVIRONMENT=local",
    "start:live": "webpack serve --open --mode development --live-reload --hot --env ENVIRONMENT=local",
    "test": "react-scripts test --watchAll",
    "test:prod": "CI=true react-scripts test --ci --watchAll=false",
    "test:local": "CI=true react-scripts test --ci --watchAll=false",
    "test:qa": "CI=true react-scripts test --ci --watchAll=false",
    "test:stage": "CI=true react-scripts test --ci --watchAll=false",
    "test:dev": "CI=true react-scripts test --ci --watchAll=false",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint --fix"
  },
  "license": "MIT",
